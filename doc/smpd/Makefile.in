ALL: all-redirect
SHELL = @SHELL@
srcdir = @srcdir@
.SUFFIXES: .pdf .dvi .tex

smpd_pmi.tex: ${srcdir}/smpd_pmi.tex
	@if test ! -s smpd_pmi.tex ; then \
		ln -s $(srcdir)/smpd_pmi.tex smpd_pmi.tex; \
	fi

smpd_tree.eps: ${srcdir}/smpd_tree.eps
	@if test ! -s smpd_tree.eps ; then \
		ln -s $(srcdir)/smpd_tree.eps smpd_tree.eps; \
	fi

smpd_pmi.dvi: smpd_pmi.tex smpd_tree.eps
	-latex smpd_pmi.tex
	-latex smpd_pmi.tex
	latex smpd_pmi.tex

smpd_pmi.pdf: smpd_pmi.dvi
	dvipdfm smpd_pmi.dvi

all-redirect: smpd_pmi.pdf

clean:
	-rm -f *.toc *.out *.dvi user.ps user.pdf *.aux *.blg *.bbl *.log

mandoc:

htmldoc:

latexdoc: ALL
