ALL: all-redirect
SHELL = @SHELL@
VPATH = @srcdir@
srcdir = @srcdir@

.SUFFIXES: .pdf .dvi .tex

@VPATH@

LATEX      = export TEXINPUTS=".:$(srcdir):" ; latex
BIBTEX     = export BIBINPUTS=".:$(srcdir):" ; bibtex
DVIPDFM    = export TEXINPUTS=".:$(srcdir):" ; dvipdfm
LATEX2HTML = latex2html

.tex.dvi:
	-$(LATEX) $< </dev/null >/dev/null 2>&1
	-$(BIBTEX) $* </dev/null
	-$(LATEX) $< </dev/null >/dev/null 2>&1
	$(LATEX) $< 

.dvi.pdf:
	$(DVIPDFM) $*

all-redirect: windev.pdf

mandoc:

htmldoc:

latexdoc: ALL

clean:
	-rm -f *.dvi *.log *.out *.aux *.toc *.bbl *.blg
